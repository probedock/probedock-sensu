{
  "checks": {
    "Partition /boot": {
      "command": "check-disk-usage.rb -I /boot -w 50 -c 80",
      "subscribers": [
        "system"
      ],
      "interval": 21600
    },
    "Partition /dev": {
      "command": "check-disk-usage.rb -I /dev -w 70 -c 90",
      "subscribers": [
        "system"
      ],
      "interval": 21600
    },
    "Partition /": {
      "command": "check-disk-usage.rb -I / -w 70 -c 90",
      "subscribers": [
        "system"
      ],
      "interval": 21600
    }
  }
}
