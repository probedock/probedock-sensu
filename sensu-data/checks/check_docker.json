{
  "checks": {
    "ProbeDock_Trial_AppContainers": {
      "command": "check_docker_container.rb -c trial_app -s 3 -wt 2 -ct 1",
      "subscribers": [
        "probedock_trial"
      ],
      "interval": 60

    },
    "ProbeDock_Demo_AppContainers": {
      "command": "check_docker_container.rb -c demo_app -s 3 -wt 2 -ct 1",
      "subscribers": [
        "probedock_demo"
      ],
      "interval": 60

    },
    "ProbeDock_Staging_AppContainers": {
      "command": "check_docker_container.rb -c staging_app -s 3 -wt 2 -ct 1",
      "subscribers": [
        "probedock_staging"
      ],
      "interval": 60
    }
  }
}
